{
  "name": "react-breadcrumb-router",
  "version": "0.2.2",
  "description": "Automatic breadcrumbs for react-router",
  "author": "FTWinston",
  "homepage": "https://github.com/FTWinston/react-breadcrumb-router",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && babel src -d dist --extensions .ts,.tsx",
    "prepare": "npm run build",
    "tidy": "npx prettier --tab-width 4 --single-quote true --write src",
    "release": "standard-version"
  },
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*"
      ]
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FTWinston/react-breadcrumb-router.git"
  },
  "bugs": {
    "url": "https://github.com/FTWinston/react-breadcrumb-router/issues"
  },
  "keywords": [
    "react-component",
    "breadcrumbs",
    "react-router",
    "react",
    "trail"
  ],
  "peerDependencies": {
    "react": "^15.0.0 || ^16.0.0 || ^17.0.0",
    "react-router-dom": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@types/history": "^4.7.9",
    "@types/react": "^17.0.27",
    "@types/react-router-dom": "^5.3.1",
    "@types/uuid": "^8.3.1",
    "babel-plugin-typescript-to-proptypes": "^1.4.2",
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.26.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.3.0",
    "standard-version": "^6.0.1",
    "typescript": "^4.4.3"
  },
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "uuid": "^8.3.2"
  }
}
